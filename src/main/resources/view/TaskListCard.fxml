<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<children>

		<GridPane HBox.hgrow="NEVER">

			<columnConstraints>
        <ColumnConstraints hgrow="NEVER" maxWidth="12.0" minWidth="12.0" prefWidth="12.0" />
				<ColumnConstraints hgrow="NEVER" maxWidth="50.0" minWidth="30.0" prefWidth="50.0" />
				<ColumnConstraints hgrow="NEVER" maxWidth="750.0" minWidth="441.0" prefWidth="687.0" />
        <ColumnConstraints maxWidth="495.0" minWidth="138.0" prefWidth="201.0" />
      <ColumnConstraints />

			</columnConstraints>
  			<rowConstraints>
  				<RowConstraints vgrow="ALWAYS" />
  			</rowConstraints>

  			<children>
  				<Label fx:id="id" maxHeight="10.0" prefHeight="10.0" styleClass="cell_big_label" GridPane.columnIndex="1" GridPane.hgrow="NEVER">
            <GridPane.margin>
              <Insets left="10.0" />
            </GridPane.margin>
          </Label>
  				
          <Label fx:id="description" prefWidth="670.0" styleClass="cell_big_label" text="\$description" wrapText="true" GridPane.columnIndex="2" GridPane.hgrow="NEVER" GridPane.vgrow="SOMETIMES">
               <padding>
                  <Insets bottom="7.0" top="7.0" />
               </padding></Label>
            <VBox alignment="CENTER" GridPane.columnIndex="3">
               <children>
          		<Label fx:id="firstDate" alignment="CENTER" contentDisplay="CENTER" maxHeight="10.0" prefHeight="10.0" prefWidth="200.0" styleClass="cell_medium_label" text="\$firstDate">
                     <padding>
                        <Insets top="7.0" />
                     </padding></Label>
            	<Label fx:id="secondDate" alignment="CENTER" contentDisplay="CENTER" maxHeight="10.0" prefHeight="10.0" prefWidth="200.0" styleClass="cell_medium_label" text="\$secondDate">
                     <padding>
                        <Insets bottom="7.0" />
                     </padding>
                  </Label>
               </children>
            </VBox>
            <VBox fx:id="colorTag" maxWidth="15.0" prefWidth="15.0" GridPane.hgrow="NEVER" />
  			</children>
        
  		</GridPane>

  	</children>
</HBox>
