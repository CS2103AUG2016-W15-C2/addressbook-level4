<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="999.0" minWidth="700.0" style="-fx-background-color: derive(#1d1d1d, 20%);" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="w15c2.tusk.ui.MainWindow">
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
   <children>
      <HBox maxHeight="10.0" prefHeight="10.0" styleClass="anchor-pane">
         <children>
            <Label fx:id="notComplete" prefHeight="5.0" style="-fx-border-radius: 10 10 10 10; -fx-background-radius: 10 10 10 10; -fx-background-color: #9999;" text="Incomplete" textFill="WHITE">
               <HBox.margin>
                  <Insets left="25.0" right="10.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <font>
                  <Font name="System Font Regular" size="18.0" />
               </font>
            </Label>
            <Label fx:id="complete" prefHeight="5.0" style="-fx-border-radius: 10 10 10 10; -fx-background-radius: 10 10 10 10;" text="Complete" textFill="WHITE">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <font>
                  <Font name="System Font Regular" size="18.0" />
               </font>
            </Label>
            <Label fx:id="find" prefHeight="5.0" style="-fx-border-radius: 10 10 0 0; -fx-background-radius: 10 10 0 0;" text="Find" textFill="WHITE">
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <font>
                  <Font name="System Font Regular" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <!--@@author A0139708W-->
      <StackPane fx:id="stackPane" styleClass="stack-pane">
         <children>
            <VBox fx:id="taskList" maxHeight="999.0" prefHeight="999.0">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="taskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox fx:id="helpPanel" maxHeight="500.0" maxWidth="600.0" prefWidth="550.0" styleClass="help-panel">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <AnchorPane fx:id="helpListPanelPlaceholder" maxWidth="600.0" />
               </children>
            </VBox>
         </children>
      </StackPane>
      <!--@@author -->
       <AnchorPane fx:id="resultDisplayPlaceholder" maxHeight="100.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
           <padding>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
           </padding>
       </AnchorPane>
      <AnchorPane fx:id="commandBoxPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
